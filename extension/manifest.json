{
    "name": "Youtube Chatbot",
    "version": "0.0.1",
    "manifest_version": 3,
    "action": {
        "default_popup": "popup.html",
        "default_icon": "logo.png"
    },
    "permissions": [
        "activeTab",
        "tabs"
    ],
    "host_permissions": [
        "http://localhost:8000/*",
        "https://www.youtube.com/*"
    ],
    "icons": {
        "128": "logo.png"
    }
}